                <div class="pageheader">
                    <h2><i class="fa fa-home"></i>Leads <span>Subtitle goes here...</span></h2>
                    <div class="breadcrumb-wrapper">
                        <span class="label">You are here:</span>
                        <ol class="breadcrumb">
                            <li><a href=" /">Bracket</a></li>
                            <li class="active">Leads</li>
                        </ol>
                    </div>
                </div>

                <div class="contentpanel">
                  <div class="row">
        
        <div class="col-md-12">
          <h5 class="subtitle mb5">Lead Details</h5>
          <p class="mb20">
            Name: <%=data.name%>, Email: <%=data.email%>, Ph. <%=data.phone%>
            <br/>
            Subject: <%=data.subject%><br/>
            Message: <%=data.message%>
         </p>
          <div class="table-responsive">
          <table class="table mb30">
            <thead>
              <tr>
                <th>#</th>
                <th>Folloup Date</th>
                <th>Remark</th>
               <th></th>
                
               
              </tr>
            </thead>
            <tbody>
             
              <% data.followup.forEach(function(x,srno){%>
              <tr>
               
                <td><%=++srno%></td>
                <td><%=x.fudate%></td>
                <td><%=x.remark%></td>
              
                
                <td class="table-action">
                  <a href=""><i class="fa fa-pencil"></i></a>
                  <a href="" class="delete-row"><i class="fa fa-trash-o"></i></a>
                </td>
               
              </tr>
              <% }); %>
            </tbody>
          </table>
          </div><!-- table-responsive -->
          <div class="panel panel-default">
        <div class="panel-heading">
          <div class="panel-btns">
            <a href="" class="panel-close">&times;</a>
            <a href="" class="minimize">&minus;</a>
          </div>
          <h4 class="panel-title">Folloup Form</h4>
          <p>Take followup from <%=data.name%> and add here.</p>
        </div>
        <div class="panel-body">
          <form class="form-inline" method="post" action="/lead-details?id=<%=data._id%>">
            <div class="form-group">
              <label class="sr-only" for="fudate">Select Date</label>
              <input type="date" class="form-control" name="fudate" id="fudate"/>
            </div>
            <div class="form-group">
              <label class="sr-only" for="remark">Remark</label>
              <input type="text" class="form-control" name="remark" id="remark" placeholder="Remark">
            </div>
            
            <button type="submit" class="btn btn-primary">Submit</button>
            <button type="reset" class="btn btn-default">Reset</button>
          </form>
        </div><!-- panel-body -->
